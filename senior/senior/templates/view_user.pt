<div metal:use-macro="main.macros['template']">
    <div metal:fill-slot="content">
    	${message}
    	 <table border = 1>
			<tr><th>Login:</th><td>${user['user_account'].login}</td></tr>
			
			<tr><th>Password:</th><td> ${user['user_account'].password} </td></tr>
			<tr><th>First Name:</th><td> ${user['user_account'].first_name} </td></tr>
			<tr><th>Middle Name:</th><td>${user['user_account'].middle_name} </td></tr>
			<tr><th>Last Name:</th><td> ${user['user_account'].last_name} </td></tr>
			<tr><th>Gender:</th><td> ${user['user_account'].gender} </td></tr>
			<tr><th>Birthday:</th><td> ${user['user_account'].birthday.day} </td></tr>
			
			<tr><th>Primary Language:</th><td> ${user['user_account'].primary_language}</td></tr>
			<tr><th>Secondary Language:</th><td> ${user['user_account'].secondary_language}</td></tr>
			<tr><th>Social Security Number:</th><td>${user['user_account'].social_security} </td></tr>
	
			<tr><th>Street:</th><td>${user['user_account'].street}, ${user['user_account'].city}, ${user['user_account'].state} ${user['user_account'].zipcode}</td></tr>
			<tr><th>Phone Number:</th><td> ${user['user_account'].phone} </td></tr>
			<tr><th>Email:</th><td> ${user['user_account'].email} </td></tr>
		</table>
    </div>
</div>